{
  "name": "bookmarked",
  "description": "A tiny (dependency free!) library for building bookmark files.",
  "version": "2.0.5",
  "author": "Ollie Williams <hey@ollie.work>",
  "bugs": {
    "url": "https://github.com/pxlprfct/bookmarked/issues"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/eslint": "^8.4.1",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/jest": "^27.4.1",
    "@types/prettier": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "ts-jest": "^26.5.6",
    "tsup": "^5.12.4",
    "typescript": "^4.6.3"
  },
  "homepage": "https://github.com/pxlprfct/bookmarked#readme",
  "keywords": [
    "bookmark",
    "chrome",
    "firefox",
    "safari"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://pxlprfct@github.com/pxlprfct/bookmarked.git"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "watch": "pnpm run build -- --watch src",
    "prepublishOnly": "pnpm run build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watchAll"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@changesets/cli": "^2.26.0"
  }
}
