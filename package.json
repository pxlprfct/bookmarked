{
  "name": "bookmarked",
  "description": "A tiny (dependency free!) library for building bookmark files.",
  "version": "3.0.2",
  "author": "Ollie Williams <hey@ollie.work>",
  "bugs": {
    "url": "https://github.com/pxlprfct/bookmarked/issues"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@types/eslint": "^8.44.2",
    "@types/eslint-config-prettier": "^6.11.0",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/lint-staged": "^13.2.0",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@vitest/coverage-v8": "^0.34.3",
    "@vitest/ui": "^0.34.3",
    "eslint": "^8.48.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vitest": "^0.3.1",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.3"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "homepage": "https://github.com/pxlprfct/bookmarked#readme",
  "keywords": [
    "bookmark",
    "chrome",
    "firefox",
    "safari"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "repository": {
    "type": "git",
    "url": "git+https://pxlprfct@github.com/pxlprfct/bookmarked.git"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --minify",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "watch": "pnpm run build -- --watch src",
    "release": "changeset publish"
  },
  "packageManager": "pnpm@8.7.0",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  }
}
