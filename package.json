{
  "name": "bookmarked",
  "description": "A tiny (dependency free!) library for building bookmark files.",
  "version": "3.0.0",
  "author": "Ollie Williams <hey@ollie.work>",
  "bugs": {
    "url": "https://github.com/pxlprfct/bookmarked/issues"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@types/eslint": "^8.4.10",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/jest": "^29.2.5",
    "@types/prettier": "^2.7.2",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "eslint": "^8.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3",
    "ts-jest": "^29.0.5",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "homepage": "https://github.com/pxlprfct/bookmarked#readme",
  "keywords": [
    "bookmark",
    "chrome",
    "firefox",
    "safari"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "repository": {
    "type": "git",
    "url": "git+https://pxlprfct@github.com/pxlprfct/bookmarked.git"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "pnpm run build",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "watch": "pnpm run build -- --watch src"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@8.7.0",
  "types": "dist/index.d.ts"
}
